    "apollo-server-cache-redis": "^1.2.2",
    "apollo-server-express": "^2.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
